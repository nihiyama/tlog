{
  "name": "tlog-vscode-extension",
  "displayName": "TLog",
  "publisher": "tlog-dev",
  "version": "0.1.0",
  "private": true,
  "description": "tlog VS Code extension",
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/tlog-dev/tlog.git"
  },
  "type": "module",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Testing"
  ],
  "activationEvents": [
    "onCommand:tlog.hello"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tlog.hello",
        "title": "TLog: Hello"
      }
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "package:vsix": "vsce package --no-dependencies",
    "publish:marketplace": "vsce publish --no-dependencies",
    "publish:precheck": "npm run typecheck && npm run test && npm run package:vsix"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@vscode/vsce": "^3.6.2"
  },
  "dependencies": {
    "@tlog/shared": "0.1.0"
  }
}
